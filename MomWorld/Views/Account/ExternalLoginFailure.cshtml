@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Mom's World - Đăng nhập thất bại</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="UTF-8" />
    <link rel="icon" type="image/ico" href="@Url.Content("~/Content/images/favicon.png")" />
    <!-- Bootstrap -->
    <link href="@Url.Content("~/Content/css/vendor/bootstrap/bootstrap.min.css")"
          rel="stylesheet">
    <link href="@Url.Content("~/Content/css/vendor/font-awesome-4.2.0/css/font-awesome.min.css")"
          rel="stylesheet">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/vendor/bootstrap-checkbox.css")">
    <link rel="stylesheet"
          href="@Url.Content("~/Content/css/vendor/bootstrap/bootstrap-dropdown-multilevel.css")">
    <link href="@Url.Content("~/Content/css/minimal.css")" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
          <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
    <style type="text/css">
        ul {
            list-style-type: none;
        }
    </style>
</head>
<body class="bg-2">
    <!-- Wrap all page content here -->
    <div id="wrap">
        <!-- Make page fluid -->
        <div class="row">
            <!-- Page content -->
            <div id="content" class="col-md-12 full-page login">

                <div class="inside-block">
                    <img src="@Url.Content("~/Content/images/logo-big.png")" class="logo img-responsive">
                    <h1>
                        <strong>Đăng nhập</strong>
                    </h1>
                    

                    <h3 class="text-danger">Đăng nhập không thành công với dịch vụ.</h3>

                </div>
            </div>
            <!-- /Page content -->
        </div>
    </div>
    <!-- Wrap all page content end -->
</body>
</html>
